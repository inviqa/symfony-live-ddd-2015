<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <embeddable name="SymfonyLive\Pos\Purchase\Purchase">
        <embedded name="price" class="SymfonyLive\Pos\Purchase\Price" use-column-prefix="false"/>
        <embedded name="sku" class="SymfonyLive\Pos\Purchase\Sku" use-column-prefix="false"/>
    </embeddable>

</doctrine-mapping>
